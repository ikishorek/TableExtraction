2 <text font="5" height="17" left="135" textpieces="0" top="732" width="4"> </text>
2 <text font="5" height="17" left="135" textpieces="0" top="770" width="4"> </text>
2 <text font="5" height="17" left="135" textpieces="0" top="809" width="4"> </text>
