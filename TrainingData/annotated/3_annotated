2 <text font="22" height="10" left="679" textpieces="0" top="746" width="13">30</text>
2 <text font="22" height="10" left="679" textpieces="0" top="720" width="13">35</text>
2 <text font="22" height="10" left="679" textpieces="0" top="693" width="13">40</text>
