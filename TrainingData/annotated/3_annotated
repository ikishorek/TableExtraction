1 <text font="5" height="17" left="306" textpieces="0" top="365" width="70">Conditions </text>
1 <text font="5" height="17" left="433" textpieces="0" top="348" width="185">Marine Algae, Pollen, Spores, </text>
1 <text font="5" height="17" left="444" textpieces="0" top="365" width="163">Leaf Waxes, Fossil Resins </text>
1 <text font="5" height="17" left="666" textpieces="0" top="348" width="50">Liquids </text>
